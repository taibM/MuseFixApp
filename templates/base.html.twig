<!DOCTYPE html>
<html lang="fr">
<head>
    {% block head %}
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta name="description" content="">
        <meta name="author" content="Tooplate">
        <link href="https://fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&display=swap" rel="stylesheet">
        <title>{% block title %}Musefix{% endblock %}</title>
    {% endblock %}
    {% block css %}
        <!-- Fichiers CSS supplémentaires -->
        <link rel="stylesheet" type="text/css" href="{{ asset('css/bootstrap.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('css/font-awesome.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('css/owl-carousel.css') }}">
        <link rel="stylesheet" href="{{ asset('css/tooplate-artxibition.css') }}">
    {% endblock %}
</head>
<body>
{% block body %}
    <!-- ***** Préchargeur Début ***** -->
    <div id="js-preloader" class="js-preloader">
        <div class="preloader-inner">
            <span class="dot"></span>
            <div class="dots">
                <span></span>
                <span></span>
                <span></span>
            </div>
        </div>
    </div>
    <!-- ***** Préchargeur Fin ***** -->

    <!-- ***** Pré-en-tête ***** -->
    <div class="pre-header">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 col-sm-6">
                    <span>Hé ! Le spectacle commence dans 15 jours.</span>
                </div>
                <div class="col-lg-6 col-sm-6">
                    <div class="text-button">
                        <a href="#">Contactez-nous maintenant ! <i class="fa fa-arrow-right"></i></a>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- ***** En-tête Début ***** -->
    <header class="header-area header-sticky">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <nav class="main-nav">
                        <!-- ***** Logo Début ***** -->
                        <a href="{{ path('app_home') }}" class="logo">Art<em>Xibition</em></a>
                        <!-- ***** Logo Fin ***** -->
                        <!-- ***** Menu Début ***** -->
                        <ul class="nav">
                            <li><a href="{{ path('app_home') }}" class="active">Accueil</a></li>
                            <li><a href="#">À propos de nous</a></li>
                            <li><a href="{{ path('app_user_index') }}">User</a></li>
                            <li><a href="#">Billets</a></li>
                        </ul>
                        <a class='menu-trigger'>
                            <span>Menu</span>
                        </a>
                        <!-- ***** Menu Fin ***** -->
                    </nav>
                </div>
            </div>
        </div>
    </header>
    <!-- ***** En-tête Fin ***** -->

    <!-- ***** Bannière Principale Début ***** -->
    <div class="main-banner">
        <div class="counter-content">
            <ul>
                <li>Jours<span id="days"></span></li>
                <li>Heures<span id="hours"></span></li>
                <li>Minutes<span id="minutes"></span></li>
                <li>Secondes<span id="seconds"></span></li>
            </ul>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="main-content">
                        <div class="next-show">
                            <i class="fa fa-arrow-up"></i>
                            <span>Prochain spectacle</span>
                        </div>
                        <h6>Ouverture le jeudi 31 mars</h6>
                        <h2>Le Sunny Hill Festival 2022</h2>
                        <div class="main-white-button">
                            <a href="#">Acheter des billets</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- ***** Bannière Principale Fin ***** -->
    <!-- *** Éléments Owl Carousel ***-->
    <div class="show-events-carousel">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="owl-show-events owl-carousel">
                        <div class="item">
                            <a href="#"><img src="{{ asset('images/show-events-01.jpg') }}" alt=""></a>
                        </div>
                        <div class="item">
                            <a href="#"><img src="{{ asset('images/show-events-02.jpg') }}"  alt=""></a>
                        </div>
                        <!-- autres éléments -->
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- autres sections -->

{% endblock %}

<!-- jQuery -->
<script src="{{ asset('js/jquery-2.1.0.min.js') }}"></script>

<!-- Bootstrap -->
<script src="{{ asset('js/popper.js') }}"></script>
<script src="{{ asset('js/bootstrap.min.js') }}"></script>

<!-- Plugins -->
<script src="{{ asset('js/scrollreveal.min.js') }}"></script>
<script src="{{ asset('js/waypoints.min.js') }}"></script>
<script src="{{ asset('js/jquery.counterup.min.js') }}"></script>
<script src="{{ asset('js/imgfix.min.js') }}"></script>
<script src="{{ asset('js/mixitup.js') }}"></script>
<script src="{{ asset('js/accordions.js') }}"></script>
<script src="{{ asset('js/owl-carousel.js') }}"></script>

<!-- Initialisation Globale -->
<script src="{{ asset('js/custom.js') }}"></script>

<div class="venue-tickets">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <div class="section-heading">
                    <h2>Nos lieux & billets</h2>
                </div>
            </div>
            <div class="col-lg-4">
                <div class="venue-item">
                    <div class="thumb">
                        <img src="{{ asset('assets/images/venue-01.jpg') }}" alt="">
                    </div>
                    <div class="down-content">
                        <div class="left-content">
                            <div class="main-white-button">
                                <a href="">Acheter des billets</a>
                            </div>
                        </div>
                        <div class="right-content">
                            <h4>Radio City Musical Hall</h4>
                            <ul>
                                <li><i class="fa fa-sitemap"></i>250</li>
                                <li><i class="fa fa-user"></i>500</li>
                            </ul>
                            <div class="price">
                                <span>1 billet<br>à partir de <em>45 $</em></span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-4">
                <div class="venue-item">
                    <div class="thumb">
                        <img src="{{ asset('assets/images/venue-02.jpg') }}" alt="">
                    </div>
                    <div class="down-content">
                        <div class="left-content">
                            <div class="main-white-button">
                                <a href="">Acheter des billets</a>
                            </div>
                        </div>
                        <div class="right-content">
                            <h4>Madison Square Garden</h4>
                            <ul>
                                <li><i class="fa fa-sitemap"></i>450</li>
                                <li><i class="fa fa-user"></i>650</li>
                            </ul>
                            <div class="price">
                                <span>1 billet<br>à partir de <em>55 $</em></span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-4">
                <div class="venue-item">
                    <div class="thumb">
                        <img src="{{ asset('assets/images/venue-03.jpg') }}" alt="">
                    </div>
                    <div class="down-content">
                        <div class="left-content">
                            <div class="main-white-button">
                                <a href="">Acheter des billets</a>
                            </div>
                        </div>
                        <div class="right-content">
                            <h4>Royce Hall</h4>
                            <ul>
                                <li><i class="fa fa-sitemap"></i>450</li>
                                <li><i class="fa fa-user"></i>750</li>
                            </ul>
                            <div class="price">
                                <span>1 billet<br>à partir de <em>65 $</em></span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</body>
